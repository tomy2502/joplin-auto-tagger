{
  "name": "joplin-plugin-ai-tag-suggester",
  "version": "1.0.2",
  "scripts": {
    "build": "webpack --env joplin-plugin-config=buildMain && webpack --env joplin-plugin-config=buildExtra",
    "dist": "npm run build && node scripts/pack-jpl.js",
    "update": "npm install -g generator-joplin && yo joplin --update",
    "prepublishOnly": "npm run dist && node scripts/pack-jpl.js && cp dist/manifest.json ./manifest.json"
  },
  "license": "MIT",
  "keywords": [
    "joplin-plugin"
  ],
  "files": [
    "dist",
    "publish",
    "manifest.json",
    "README.md",
    "LICENSE"
  ],
  "homepage": "https://github.com/cenktekin/joplin-auto-tagger#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cenktekin/joplin-auto-tagger.git"
  },
  "bugs": {
    "url": "https://github.com/cenktekin/joplin-auto-tagger/issues"
  },
  "devDependencies": {
    "@joplin/tools": "^2.13.0",
    "@types/node": "^18.7.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "glob": "^8.0.3",
    "html-webpack-plugin": "^5.6.0",
    "on-build-webpack": "^0.1.0",
    "tar": "^6.1.11",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@google/genai": "^1.15.0",
    "gaxios": "^6.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
